#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
int main(){

    ll T;cin>>T;
    for(ll j=0;j<T;j++){
        ll n,k,b,t;
        cin>>n>>k>>b>>t;

        ll x[n],v[n];

        for(ll i=0;i<n;i++) cin>>x[i];
        for(ll i=0;i<n;i++) cin>>v[i];

        ll count=0,swaps=0,not_possible=0;

        for(ll i=n-1;i>=0;i--){
                ll dis_req = b - x[i];
                ll dis_cov = v[i]*t;

                if(dis_cov >= dis_req){
                    count++;
                    if(not_possible>0) swaps+=not_possible;
                }else{
                    not_possible++;
                }

                if(count>=k) break;
        }

        if(count>=k) cout<<"Case #"<<j+1<<": "<<swaps<<endl;
        else cout<<"Case #"<<j+1<<": IMPOSSIBLE"<<endl;

    }
    return 0;
}
